{
    "name": "simonefuoco/tappy",
    "description": "WordPress fork starting from 5.2.3 release.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Tappy\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-xdebug": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "phpunit/phpunit": "8.4.1"
    }
}
